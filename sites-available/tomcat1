server {
  listen 80;

  server_name    tomcat1.setranspe.net;
  access_log /var/log/nginx/node-postovemvirtualfront-access.log;
  error_log /var/log/nginx/node-postovemvirtualfront-error.log;

  location / {
        proxy_set_header X-Forwarded-Host $host;
        proxy_set_header X-Forwarded-Server $host;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Real-IP $remote_addr;
	proxy_pass http://172.16.20.10:8080;
  }
}


