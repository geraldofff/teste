server {
  listen 80;

  server_name    tomcat2.setranspe.net;
  access_log /var/log/nginx/tomcat-postovirtualwebsocial-access.log;
  error_log /var/log/nginx/tomcat-postovirtualwebsocial-error.log;

  location / {
        proxy_set_header X-Forwarded-Host $host;
        proxy_set_header X-Forwarded-Server $host;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Real-IP $remote_addr;
	proxy_pass  http://172.16.20.40:8080;
  }
}


